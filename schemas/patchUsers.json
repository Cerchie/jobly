{
    "$schema": "http://json-schema.org/draft-07/schema",
    "$id": "http://example.com/example.json",
    "type": "object",
    "title": "The root schema",
    "description": "The root schema comprises the entire JSON document.",
    "default": {},
    "examples": [
        {
            "username": "Fishy",
            "password": "iEeg134$3",
            "first_name": "Fish",
            "last_name": "Fisherson",
            "email": "fish@gmail.com",
            "photo_url": "https://freeiconshop.com/wp-content/uploads/edd/pear-outline-filled.png",
            "is_admin": "false"
        }
    ],
    "properties": {
        "username": {
            "$id": "#/properties/username",
            "type": "string",
            "title": "The username schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Fishy"
            ]
        },
        "password": {
            "$id": "#/properties/password",
            "type": "string",
            "title": "The password schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "iEeg134$3"
            ]
        },
        "first_name": {
            "$id": "#/properties/first_name",
            "type": "string",
            "title": "The first_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Fish"
            ]
        },
        "last_name": {
            "$id": "#/properties/last_name",
            "type": "string",
            "title": "The last_name schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "Fisherson"
            ]
        },
        "email": {
            "$id": "#/properties/email",
            "type": "string",
            "title": "The email schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "fish@gmail.com"
            ]
        },
        "photo_url": {
            "$id": "#/properties/photo_url",
            "type": "string",
            "title": "The photo_url schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "https://freeiconshop.com/wp-content/uploads/edd/pear-outline-filled.png"
            ]
        },
        "is_admin": {
            "$id": "#/properties/is_admin",
            "type": "string",
            "title": "The is_admin schema",
            "description": "An explanation about the purpose of this instance.",
            "default": "",
            "examples": [
                "false"
            ]
        },
        "_token": {
            "$id": "/properties/token",
            "type": "string",
            "title": "The Token Schema ",
            "default": "",
            "examples": [
              "foo123"
            ]
          }
    },
    "additionalProperties": true
}